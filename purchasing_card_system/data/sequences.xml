<odoo>
  <data>
        <record id="seq_card_contract" model="ir.sequence">
            <field name="name">Card COntract</field>
            <field name="code">card.contract</field>
            <field name="prefix">Contract/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_card_purchase" model="ir.sequence">
            <field name="name">Card purchase</field>
            <field name="code">card.purchase</field>
            <field name="prefix">Purchase/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
  </data>
</odoo>